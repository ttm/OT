@prefix ot: <http://purl.org/socialparticipation/ot/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ot:ComplexNetworks a owl:Class ;
    rdfs:label "Complex Networks"@en,
        "Redes Complexas"@pt ;
    rdfs:subClassOf ot:StatisticalPhysics .

ot:GraphTheory a owl:Class ;
    rdfs:label "Graph Theory"@en,
        "Teoria de Grafos"@pt .

ot:Probability a owl:Class ;
    rdfs:label "Probability"@en,
        "Probabilidade"@pt .

ot:grounds a owl:ObjectProperty ;
    rdfs:label "fundamenta"@pt .

ot:StatisticalPhysics a owl:Class ;
    rdfs:label "Statistical Physics"@en,
        "Física Estatística"@pt .

